{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "interactivity-api-examples/quiz",
	"version": "1.0.0",
	"title": "Quiz",
	"category": "widgets",
	"icon": "smiley",
	"description": "Make a quiz",
	"attributes": {
		"question": {
			"type": "string"
		},
		"typeOfQuiz": {
			"type": "string",
			"enum": ["boolean", "input"],
			"default": "boolean"
		},
		"answer": {
			"type": ["string", "boolean"],
			"default": true
		}
	},
	"textdomain": "interactivity-api-examples",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
